\newcommand{\cdx@studentcontent}[2]{%
    \begin{tabular}{c}%
        \ifx\cdx@pagestyle\cdx@fancydark%
        \color{CoDeXTextOnDark}%
        \else%
        \color{CoDeXTextOnLight}%
        \fi%
        \textbf{\normalsize #1}\\
        \vspace{0.5ex}%
        \ifx\cdx@pagestyle\cdx@fancydark%
        \color{CoDeXTextOnDark}%
        \else%
        \color{CoDeXTextOnLight}%
        \fi%
        \small \cdx@studentID: #2%
    \end{tabular}%
}%

